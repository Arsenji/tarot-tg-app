{
  "name": "tarot-tg-app",
  "version": "1.0.0",
  "description": "Telegram Tarot Application - Frontend, Backend, Admin Panel",
  "private": true,
  "workspaces": [
    "frontend",
    "backend", 
    "admin-backend"
  ],
  "scripts": {
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev", 
    "dev:admin-backend": "cd admin-backend && npm run dev",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:admin-backend\"",
    "build": "tsc",
    "build:all": "npm run build:frontend && npm run build:backend && npm run build:admin-backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "build:admin-backend": "cd admin-backend && npm run build", 
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install && cd ../admin-backend && npm install",
    "clean": "npm run clean:frontend && npm run clean:backend && npm run clean:admin-backend",
    "clean:frontend": "cd frontend && rm -rf .next out node_modules",
    "clean:backend": "cd backend && rm -rf dist node_modules",
    "clean:admin-backend": "cd admin-backend && rm -rf dist node_modules",
    "lint": "npm run lint:frontend && npm run lint:backend && npm run lint:admin-backend",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && echo \"No linting configured for backend\"",
    "lint:admin-backend": "cd admin-backend && echo \"No linting configured for admin-backend\""
  },
  "keywords": [
    "tarot",
    "telegram", 
    "bot",
    "telegram-webapp",
    "typescript",
    "react",
    "nextjs",
    "express",
    "mongodb",
    "openai",
    "admin-panel"
  ],
  "author": "Tarot Telegram App Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.4.5",
    "mongoose": "^8.0.0",
    "telegraf": "^4.12.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "express-mongo-sanitize": "^2.2.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.6.0",
    "node-telegram-bot-api": "^0.64.0",
    "node-fetch": "^3.3.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "openai": "^4.20.0",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "object-hash": "^3.0.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "typescript": "^5.3.3",
    "@types/node": "^20.11.24",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.10.9"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/tarot-tg-app.git"
  }
}